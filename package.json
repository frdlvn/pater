{
  "name": "pater",
  "version": "0.1.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "test": "vitest run --config vite.config.js \"test/**/*.test.js\"",
    "build:win": "electron-builder --win --publish=never",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.pater.app",
