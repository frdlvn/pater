    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "private": "true",
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@langchain/langgraph": "^0.4.9",
    "electron-store": "^9.0.0",
    "node-cron": "^3.0.3"
  }
}
